def walk:
   return 'walking'

def bus:
   return 'taking a bus'

def car:
   return 'commuting by car'

class Movement:
    def __init__(self, strategy):
        self.strategy = strategy

    def move(self):
        return self.strategy()

movement = Movement(walk)
print(movement.move())

movement.strtegy = bus
print(movement.move())

movement.strategy = car
print(movement.move())

